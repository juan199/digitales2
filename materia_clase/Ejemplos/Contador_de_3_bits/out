#! /usr/local/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7-25-ge162258)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1db5b20 .scope module, "BancoPruebas" "BancoPruebas" 2 1;
 .timescale 0 0;
v0x1dc4e30_0 .net "Q", 2 0, v0x1db5070_0; 1 drivers
v0x1dc4ee0_0 .net "clk", 0 0, v0x1dc4d60_0; 1 drivers
S_0x1dc4c70 .scope module, "r1" "relojito" 2 5, 2 18, S_0x1db5b20;
 .timescale 0 0;
v0x1dc4d60_0 .var "reloj", 0 0;
S_0x1db5c10 .scope module, "c1" "cont3bC" 2 6, 2 28, S_0x1db5b20;
 .timescale 0 0;
v0x1db5070_0 .var "Q", 2 0;
v0x1dc4bd0_0 .alias "reloj", 0 0, v0x1dc4ee0_0;
E_0x1d91c20 .event posedge, v0x1dc4bd0_0;
    .scope S_0x1dc4c70;
T_0 ;
    %delay 30, 0;
    %set/v v0x1dc4d60_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1dc4c70;
T_1 ;
    %delay 6, 0;
    %load/v 8, v0x1dc4d60_0, 1;
    %inv 8, 1;
    %set/v v0x1dc4d60_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1db5c10;
T_2 ;
    %set/v v0x1db5070_0, 0, 3;
    %end;
    .thread T_2;
    .scope S_0x1db5c10;
T_3 ;
    %wait E_0x1d91c20;
    %delay 5, 0;
    %load/v 8, v0x1db5070_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %set/v v0x1db5070_0, 8, 3;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1db5b20;
T_4 ;
    %vpi_call 2 10 "$dumpfile", "senales.vcd";
    %vpi_call 2 11 "$dumpvars";
    %vpi_call 2 12 "$display", "Hola Mundo!!";
    %vpi_call 2 13 "$monitor", $time, " ", "clk = %b", v0x1dc4ee0_0;
    %delay 500, 0;
    %vpi_call 2 14 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Contador3bits.v";
